<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dmcapps.navigationfragmentexample" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.dmcapps.navigationfragmentexample.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.dmcapps.navigationfragmentexample.SingleStackExample.SingleStackNavigationExampleActivity" />
        <activity android:name="com.dmcapps.navigationfragmentexample.MasterDetailExample.MasterDetailNavigationExampleActivity" >
        </activity>
    </application>

</manifest>
